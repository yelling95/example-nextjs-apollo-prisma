// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  uuid  String  @id @default(uuid())
  id    String  @unique
  email String  @unique
  name  String?
}
